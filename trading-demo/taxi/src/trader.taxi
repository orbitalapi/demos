namespace com.lunarbank.demo

@Table(connection = "referenceDataDb", schema = "public" , table = "traders")
parameter model TraderRecord {
    @Id id : TraderId
    name: TraderFirstName
    lastName: TraderLastName
    deskId: DeskId
}


@DatabaseService( connection = "referenceDataDb" )
service TradersDb {
   table trader : TraderRecord[]
}
