namespace com.orbital.hzdemo

@Table(connection = "demoDb", schema = "public" , table = "traders")
parameter model TraderRecord {
    @Id id : TraderId
    name: TraderName
    lastName: TraderLastName
    deskId: DeskId
}


@DatabaseService( connection = "demoDb" )
service TradersDb {
   table trader : TraderRecord[]
}